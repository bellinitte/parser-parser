{
  "name": "parser-parser",
  "version": "0.1.0",
  "description": "EBNF production rule parsing tool",
  "author": "Karol Belina <karolbelina@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/karolbelina/parser-parser.git"
  },
