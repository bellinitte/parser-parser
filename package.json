    "@wasm-tool/rollup-plugin-rust": "^1.0.0",
    "babel-jest": "^26.2.2",
    "gh-pages": "^3.1.0",
    "jest": "^26.1.0",
    "jest-transform-svelte": "^2.1.1",
    "prettier": "^2.1.2",
